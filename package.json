    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.2.1",
    "@aws-amplify/backend-cli": "^1.2.5",
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "aws-cdk": "^2.156.0",
    "aws-cdk-lib": "^2.156.0",
    "constructs": "^10.3.0",
