{
  "name": "kilowattsgrid",
  "version": "1.0.0",
  "main": "index",
  "scheme": "kilowattsgrid",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export -p web",
    "serve": "npm run build:web && npx serve dist",
    "precommit": "lint-staged && jest",
    "format": "prettier --write 'src'",
    "postinstall": "npx setup-skia-web && cp public/_expo/static/js/web/canvaskit.wasm public/canvaskit.wasm"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.5",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "@shopify/flash-list": "^1.6.4",
    "@shopify/react-native-skia": "1.2.3",
    "aws-amplify": "^6.6.0",
    "expo": "~51.0.28",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "^0.25.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-error-boundary": "^1.2.4",
